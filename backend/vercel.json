{
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python"
        },
        {
            "src": "src/db_actions.py",
            "use": "@vercel/python"
        },
        {
            "src": "src/defs.py",
            "use": "@vercel/python"
        },
        {
            "src": "src/google_requests.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }

    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ]
}